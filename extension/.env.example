# Chrome 扩展发布
CHROME_EXTENSION_ID=your-chrome-extension-id
CHROME_CLIENT_ID=your-chrome-client-id
CHROME_CLIENT_SECRET=your-chrome-client-secret
CHROME_REFRESH_TOKEN=your-chrome-refresh-token

# Firefox 扩展发布
FIREFOX_EXTENSION_ID=your-firefox-extension-id
FIREFOX_EXTENSION_SLUG=your-firefox-extension-slug
FIREFOX_API_KEY=your-firefox-api-key
FIREFOX_API_SECRET=your-firefox-api-secret

# Edge 扩展发布
EDGE_PRODUCT_ID=your-edge-product-id
EDGE_CLIENT_ID=your-edge-client-id
EDGE_CLIENT_SECRET=your-edge-client-secret
EDGE_ACCESS_TOKEN_URL=https://login.microsoftonline.com/your-tenant-id/oauth2/v2.0/token

# API设置
# 设置后端API的URL，开发环境默认为localhost
API_URL=http://localhost:8000/api

# 在生产环境中，请更新为您的实际API URL
# API_URL=https://your-production-api.com/api 